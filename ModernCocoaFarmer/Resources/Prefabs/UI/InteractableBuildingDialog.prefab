<?xml version="1.0" encoding="utf-8"?>
<Prefab>
  <GameObjects>
    <GameObject name="InteractableBuildingPopup" active="true" should_render="true" position="0.5%,0.5%">
      <Components>
        <Canvas projection_mode="Orthographic" dimensions="1%,1%"/>
        <SpriteRenderer sprite="Textures\UI\Utility\WhitePixel.png" size="0.3%,0.3%,0" colour="0.8,0.8,0.8" opacity="0.5"/>
      </Components>
      <GameObjects>
        <GameObject name="TitleText" position="0,0.12%,0">
          <Components>
            <TextRenderer font_height="30"/>
          </Components>
        </GameObject>
        <GameObject name="DescriptionText" position="0,0,0" horizontal_alignment="Centre" vertical_alignment="Centre">
          <Components>
            <TextRenderer font_height="20"/>
          </Components>
        </GameObject>
        <GameObject name="HelpText" position="0,-0.075%,0" horizontal_alignment="Centre" vertical_alignment="Centre">
          <Components>
            <TextRenderer text="Select a child using the icons at the top and click on this building to send them here."
                          font_height="20"/>
          </Components>
        </GameObject>
        <GameObject name="CloseButton" position="0.15%, 0.15%">
          <Components>
            <SpriteRenderer sprite="Textures\UI\Utility\Cross.png" size="50,50"/>
            <RectangleCollider dimensions="50,50"/>
            <MouseInteractionHandler/>
          </Components>
        </GameObject>
        <GameObject name="StatsBackground" position="-0.075%,-0.2%,0">
          <Components>
            <SpriteRenderer sprite="Textures\UI\Utility\WhitePixel.png" size="0.15%, 0.1%" colour="0.8,0.8,0.8" opacity="0.5"/>
          </Components>
        </GameObject>
      </GameObjects>
    </GameObject>
  </GameObjects>
</Prefab>