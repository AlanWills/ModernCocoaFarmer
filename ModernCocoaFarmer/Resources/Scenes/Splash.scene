<?xml version="1.0" encoding="utf-8"?>
<Scene>
  <Resources>
    <Texture2Ds>
      <Texture2D>Textures\Scenes\SplashScene.png</Texture2D>
    </Texture2Ds>
  </Resources>
  <GameObjects>
    <GameObject name="SplashScene">
      <Components>
        <Canvas projection_mode="Orthographic" dimensions="1920,1080"/>
      </Components>
      <GameObjects>
        <GameObject position="0.5, 0.5, 0" name="Logo1" active="true" should_render="true">
          <Components>
            <SpriteRenderer sprite="Textures\Scenes\SplashSmall.png" size="1920,1080" opacity="0" preserve_aspect_ratio="true"/>
            <OpacityLerper lerping_up="true" lerp_down_time="0" lerp_up_time="1" min_wait_time="0" max_wait_time="2.2"/>
          </Components>
        </GameObject>
        <GameObject name="SceneTransitioner">
          <Components>
            <EventTriggerer trigger_delay="3.2" trigger_key="esc"/>
          </Components>
        </GameObject>
      </GameObjects>
    </GameObject>
  </GameObjects>
</Scene>
